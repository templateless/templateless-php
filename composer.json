{
    "name": "templateless/templateless",
    "description": "Ship faster by sending elegant emails using just code",
    "license": "MIT",
    "keywords": [
        "templateless",
        "email",
        "templates"
    ],
    "authors": [],
    "homepage": "https://templateless.com",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "suggest": {},
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Templateless\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Templateless\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    }
}
